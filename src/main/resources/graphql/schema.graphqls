type Query {
    getUserById(id: ID): UserResponse
    getAllUsers: [UserResponse]

}

type Mutation {

    #Auth
    register(userRegisterRequest: UserRegisterRequest): JwtResponse
    login(jwtRequest: UserLoginRequest): JwtResponse
    refresh(refreshTokenRequest: UserRefreshRequest): JwtResponse

    #User
    updateUser(userUpdateRequest: UserUpdateRequest): UserResponse
    deleteUser(id: ID): Boolean

}